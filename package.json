{
  "name": "@mattdeveloper/native-ui",
  "version": "0.1.0",
  "private": false,
  "description": "React Native UI components designed for seamless integration and enhanced mobile app development.",
  "main": "lib/commonjs/index.js",
  "author": "Matt Oliveira <talk@mattdeveloper.com> (https://github.com/mattdeveloper)",
  "license": "MIT",
  "homepage": "https://github.com/@mattdeveloper/native-ui#readme",
  "devDependencies": {
    "react-native-builder-bob": "^0.23.2",
    "styled-components": "^6.1.10",
    "typescript": "^5.4.5"
  },
  "react-native": "src/index.tsx",
  "source": "src/index.tsx",
  "module": "lib/module/index.js",
  "types": "lib/typescript/src/index.d.ts",
  "scripts": {
    "prepare": "bob build"
  },
  "peerDependencies": {
    "react": "^18.2.0",
